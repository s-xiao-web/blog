.anchor-wrapper {
  position: absolute;
  top: 100px;
  right: -200px;
}
.anchor-wrapper :global .ant-anchor-wrapper {
  background: transparent;
}
.anchor-wrapper :global .ant-anchor-link-title {
  color: #999 !important;
}
.anchor-wrapper :global .ant-anchor-link-title-active {
  color: #1890ff !important;
}
